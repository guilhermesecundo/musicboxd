#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules/node-gyp-build/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/node_modules:/mnt/c/Users/guilh/Área de Trabalho/node_modules:/mnt/c/Users/guilh/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules/node-gyp-build/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/node_modules:/mnt/c/Users/guilh/Área de Trabalho/node_modules:/mnt/c/Users/guilh/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/guilh/Área de Trabalho/Documents/dev/ep's/musicboxd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/build-test.js" "$@"
else
  exec node  "$basedir/../node-gyp-build/build-test.js" "$@"
fi
